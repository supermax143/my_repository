<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/3.7">
	<id>com.feathersui.examples.Todos</id>
	<filename>Todos</filename>
	<name>Todos</name>
	<versionNumber>1.1.0</versionNumber>
	<description>Todos example application built with Feathers UI controls for Starling</description>
	<copyright>2013 Josh Tynjala</copyright>

	<initialWindow>
		<content>TodosMobile.swf</content>
		<autoOrients>true</autoOrients>
		<fullScreen>true</fullScreen>
		<visible>true</visible>
		<renderMode>direct</renderMode>
	</initialWindow>

	

	<android>
		<colorDepth>16bit</colorDepth>
		<manifestAdditions><![CDATA[
			<manifest android:installLocation="auto">
			    <uses-permission android:name="android.permission.INTERNET"/>
			</manifest>
		]]></manifestAdditions>
	</android>
	<iPhone>
		<InfoAdditions>
		<![CDATA[
			<key>UIDeviceFamily</key> 
			<array> 
				<string>1</string>
				<string>2</string>
			</array>
			<key>UIPrerenderedIcon</key>
			<true/>
		]]> 
		</InfoAdditions>
		<requestedDisplayResolution>high</requestedDisplayResolution>
	</iPhone>
</application>